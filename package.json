{
  "name": "flochat",
  "version": "0.1.0",
  "description": "A whatsapp type decentralized chat application based on FLO Blockchain ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify public/js/main.js -o public/js/bundle.js",
    "watch": "watchify public/js/main.js -o public/js/bundle.js",
    "prestart": "npm run build",
    "start": "nodemon --ignore public/js/bundle.js index.js",
    "poststart": "npm run watch"
  },
  "engines": {
    "node": "8.11.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avishkarabhishek786/flochat.git"
  },
  "keywords": [
    "FLO",
    "chat",
    "application",
    "Blockchain",
    "Node",
    "JS",
    "Electron",
    "JS"
  ],
  "author": "Abhishek Sinha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avishkarabhishek786/flochat/issues"
  },
  "homepage": "https://github.com/avishkarabhishek786/flochat#readme",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "popper.js": "^1.14.4",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "nodemon": "^1.18.3",
    "watchify": "^3.11.0"
  }
