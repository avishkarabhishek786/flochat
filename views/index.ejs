        <div class="row">
                <div class="col-md-12">
                        <ul id="chat-ul"></ul>
                </div>
        </div>

        <div class="row">
                <div class="col-md-12">
                        <form>
                                <h5 class="mb-3">Chat</h5>

                                <div class="form-group row">
                                        <div class="col-sm-10">
                                                <input type="text" class="form-control" id="text-msg" placeholder="Your message">
                                        </div>
                                </div>

                                <div class="form-group row">
                                        <div class="col-sm-10">
                                                <button type="submit" id="btn-chat" class="btn btn-primary">Send Message</button>
                                                <button id="geo-btn" class="btn btn-warning">Send Location</button>
                                        </div>
                                </div>
                        </form>
                </div>
        </div>
    </div>
        
